{
  "name": "priede-langsupport",
  "displayName": "priede-langsupport",
  "description": "Priedes programmēšanas valodas atbalsts",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "priede",
      "aliases": ["Priede", "priede"],
      "extensions": [".pr"],
      "configuration": "./language-configuration.json",
      "icon": {"light": "priede-dark.png", "dark": "priede-light.png"}
    }],
    "grammars": [{
      "language": "priede",
      "scopeName": "text.priede",
      "path": "./syntaxes/priede.tmLanguage.json"
    }]
  }
}
